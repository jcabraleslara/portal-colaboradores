            "typeVersion": 1,
            "position": [
                -900,
                400
            ],
            "id": "supabase",
            "credentials": {
                "supabaseApi": {
                    "id": "OfStvPP3jSJ53nzk",
                    "name": "Supabase account"
                }
            }
