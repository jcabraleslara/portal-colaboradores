{
    "name": "Sync 0 - Limpieza Duplicados Google V3 (FINAL)",
    "nodes": [
        {
            "parameters": {},
            "name": "Ejecutar Limpieza",
            "type": "n8n-nodes-base.manualTrigger",
            "typeVersion": 1,
            "position": [
                0,
                0
            ],
            "id": "trigger"
        },
        {
            "parameters": {
                "url": "https://people.googleapis.com/v1/people/me/connections",
                "authentication": "predefinedCredentialType",
                "nodeCredentialType": "googleContactsOAuth2Api",
                "sendQuery": true,
                "queryParameters": {
                    "parameters": [
                        {
                            "name": "personFields",
                            "value": "names"
                        },
                        {
                            "name": "pageSize",
                            "value": "2000"
                        }
                    ]
                }
            },
            "name": "1. Obtener Todos",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 4,
            "position": [
                220,
                0
            ],
            "id": "get-all",
            "credentials": {
                "googleContactsOAuth2Api": {
                    "id": "uF3bSNDa0djQ8pTu",
                    "name": "Google Contacts account"
                }
            }
        },
        {
            "parameters": {
                "jsCode": "const connections = $input.first().json.connections || [];\nconst seen = new Map();\nconst duplicates = [];\n\nfunction clean(s) {\n  if (!s) return '';\n  return s.toString().toLowerCase().normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\").replace(/[^a-z0-9]/g, \"\").trim();\n}\n\nconnections.forEach(person => {\n  const names = person.names || [];\n  if (names.length > 0) {\n    const key = clean(names[0].displayName || `${names[0].givenName || ''}${names[0].familyName || ''}`);\n    if (key.length > 3) {\n      if (seen.has(key)) {\n        duplicates.push({ resourceName: person.resourceName });\n      } else {\n        seen.set(key, true);\n      }\n    }\n  }\n});\n\n// Retornar la lista de duplicados para el nodo Split\nreturn duplicates.map(d => ({ json: d }));"
            },
            "name": "2. Listar Duplicados",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                440,
                0
            ],
            "id": "list-dupes"
        },
        {
            "parameters": {
                "batchSize": 50,\n        "options": {}\n
            },
            "name": "3. Grupos de 50",
            "type": "n8n-nodes-base.splitInBatches",
            "typeVersion": 3,
            "position": [
                660,
                0
            ],
            "id": "batch"
        },
        {
            "parameters": {
                "method": "POST",
                "url": "https://people.googleapis.com/v1/people:batchDeleteContacts",
                "authentication": "predefinedCredentialType",
                "nodeCredentialType": "googleContactsOAuth2Api",
                "sendBody": true,
                "specifyBody": "json",
                "jsonBody": "={\n  \"resourceNames\": {{ JSON.stringify($input.all().map(i => i.json.resourceName)) }}\n}",
                "options": {}
            },
            "name": "4. Borrar Lote",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 4,
            "position": [
                900,
                0
            ],
            "id": "delete-batch",
            "credentials": {
                "googleContactsOAuth2Api": {
                    "id": "uF3bSNDa0djQ8pTu",
                    "name": "Google Contacts account"
                }
            }
        },
        {
            "parameters": {
                "jsCode": "return [{ json: { msg: \"Bote de 50 borrado o finalizado.\" } }];"
            },
            "name": "5. Continuar",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                1150,
                0
            ],
            "id": "continue"
        }
    ],
