        {
            "parameters": {
                "jsCode": "// Guardar contactos de Google\nconst resp = $input.first().json;\nglobalThis.googleContacts = resp.connections || [];\nconsole.log(`Google: ${globalThis.googleContacts.length} contactos`);\nreturn [{ json: { ok: true, count: globalThis.googleContacts.length } }];"
